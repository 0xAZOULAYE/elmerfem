Check Keywords "Warn"

Header
  Mesh DB "." "cube2"
End

Simulation
  Max Output Level = 3
  Coordinate System = Cartesian
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
  Post File   = "ptest.ep"
  Output File = "ptest.dat"
End

Body 1
  Equation = 1
  Body Force = 1
End

Body 2
  Equation = 1
  Body Force = 1
End

Body Force 1 :: Source = Real 1
Equation 1 :: Active Solvers(1) = 1

Solver 1
  Equation = "Poisson"

  Variable = "Potential"
  Variable DOFs = 1
  Procedure = "PoissonOMPBLAS" "PoissonSolver"

  ! Element = "p:2"

  !  Linear System Solver = "Direct"
  Linear System Symmetric = True
  Linear System Solver = "Iterative"
  Linear System Iterative Method = Cg
  Linear System Convergence Tolerance = 1e-6
  Linear System Preconditioning = None
  Linear System Max Iterations = 200
  Optimize Bandwidth = False
  ! Linear System Residual Output = 1
  Steady State Convergence Tolerance = 1e-09
  Bubbles in Global System = False
 
  ! Linear System Save = Logical True
  ! Linear System Save Prefix = File "ptest"
  ! Linear System Save Format = String "MatrixMarket"
  ! Linear System Save and Stop = Logical False
End

Boundary Condition 1
  Target Boundaries(4) = 1 2 3 4
  Potential = Real 0
End

$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=0.223544023689E-01\n" );
