Check Keywords "Warn"

Header
  Mesh DB "." "mesh3d"
End

Simulation
  Max Output Level = 9

  Coordinate System = "Cartesian"
  Simulation Type = Steady
  Timestepping Method = BDF
  Timestep Sizes = 1
  Timestep Intervals = 10
  Steady State Max Iterations = 1

  Post File = MGDynamics.vtu
  Output Intervals(1) = 1
End

Constants
 Permeability of Vacuum = Real 1
 Permittivity of Vacuum = Real 1
End

Body 1
  Equation = 1
  Material = 1
  Body Force = 1
  Initial Condition = 1
End

Initial Condition 1
! Pre = Variable coordinate
!  Real MATC "rand(1)"
End

Material 1
  Reluctivity  = Real 1
  Permittivity = Real 1
  !Permeability = Real 1
  Electric Conductivity = Real 1
End

Equation 1
  Active Solvers(2) = 1 2
End

$ w=2

Solver 1
  Equation = "VectorHelmholtz"
  Variable = A[A re:1 A im:1]

  Procedure = "VectorHelmholtz" "WhitneyAVHarmonicSolver"

  Angular Frequency = Real $w

  Use Tree Gauge = Logical False

  Linear System Solver = String "Iterative"
  Linear System Iterative Method = String "bicgstab2"
  BiCGStabl Polynomial Degree = 2
  Linear System Preconditioning = ILU0
  Linear System Max Iterations = Integer 1000
  Linear System Convergence Tolerance = 1.0e-4
  Linear System Residual Output = 10

  Steady State Convergence Tolerance = 1e-09
  Automated Source Projection BCs = Logical False
  Calculate Loads = Logical True
  Calculate Energy Norm = Logical True
End

Solver 2
  Equation = "calcfields"

  Procedure = "VectorHelmholtz" "MagnetoDynamicsCalcFields"
  Linear System Symmetric = True

  Potential variable = String "A"

  Angular Frequency = Real $w

  Calculate Elemental Fields = Logical False
  Calculate Current Density = Logical False
  Calculate Electric Field = Logical False
  Calculate Magnetic Field Strength = Logical False
  Calculate Magnetic Vector Potential = Logical True

  Steady State Convergence Tolerance = 1
  Linear System Solver = "Direct"
  Linear System Preconditioning = None
  Linear System Residual Output = 0
  Linear System Max Iterations = 5000
  Linear System Iterative Method = CG
  Linear System Convergence Tolerance = 1.0e-6
End


Body Force 1
  Current Density 1 = Real 0
  Current Density 2 = Real 0
  Current Density 3 = Real 0
End

Boundary Condition 1
  Target Boundaries(5) = 1 2 3 4 5

  A re {e} = Real 0
  A im {e} = Real 0
End
 
Boundary Condition 2
  Target Boundaries(1) = 6

  A re {e} 1 = Real 0
  A im {e} 1 = Real 0
  A re {e} 2 = Variable Coordinate 1
  Real MATC "sin(pi*tx)"
! Tai:
! Variable Coordinate
! Real MATC "sin(pi*tx(0))"
  A im {e} 2 = Real 0
  A re {e} 3 = Real 0
  A im {e} 3 = Real 0
End

Solver 1 :: Reference Norm = Real 1
Solver 1 :: Reference Norm Tolerance = Real 1e-3
$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=1,EPS=1e-3\n" );
